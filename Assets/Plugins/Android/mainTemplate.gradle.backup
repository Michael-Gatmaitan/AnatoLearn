apply plugin: 'com.android.library'
apply from: 'setupSymbols.gradle'
apply from: '../shared/keepUnitySymbols.gradle'

dependencies {
    implementation project(':unityLibrary')
}

android {
    namespace "com.UnityTechnologies.com.unity.template.urpblank"
    ndkPath "C:/Program Files/Unity/Hub/Editor/6000.0.41f1/Editor/Data/PlaybackEngines/AndroidPlayer/NDK"
    ndkVersion "27.2.12479018"
    compileSdk 33
    buildToolsVersion = "34.0.0"

    compileOptions {
        sourceCompatibility JavaVersion.VERSION_17
        targetCompatibility JavaVersion.VERSION_17
    }

    defaultConfig {
**DEFAULT_CONFIG_SETUP**
consumerProguardFiles 'proguard-unity.txt'**USER_PROGUARD**
        applicationId "com.UnityTechnologies.com.unity.template.urpblank"
